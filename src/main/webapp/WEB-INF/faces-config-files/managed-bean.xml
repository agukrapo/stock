<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>loginController</managed-bean-name>
        <managed-bean-class>ar.com.ak.bean.LoginController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>applicationController</managed-bean-name>
        <managed-bean-class>ar.com.ak.bean.ApplicationController</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>sessionController</managed-bean-name>
        <managed-bean-class>ar.com.ak.bean.SessionController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>talleController</managed-bean-name>
        <managed-bean-class>ar.com.ak.bean.TalleController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>usuarioController</managed-bean-name>
        <managed-bean-class>ar.com.ak.bean.UsuarioController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>categoriaController</managed-bean-name>
        <managed-bean-class>ar.com.ak.bean.CategoriaController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>empresaController</managed-bean-name>
        <managed-bean-class>ar.com.ak.bean.EmpresaController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>parametrosStockController</managed-bean-name>
        <managed-bean-class>ar.com.ak.bean.ParametrosStockController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>articuloController</managed-bean-name>
        <managed-bean-class>ar.com.ak.bean.ArticuloController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>procesarArticuloController</managed-bean-name>
        <managed-bean-class>ar.com.ak.bean.ProcesarArticuloController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>transaccionController</managed-bean-name>
        <managed-bean-class>ar.com.ak.bean.TransaccionController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>
